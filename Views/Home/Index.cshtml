@{
    ViewData["Title"] = "Home Page";
}

<style>
    html, body {
        height: 100%;
    }

    .carousel-item img {
        object-fit: fill;
        height: 76vh;
        width: 100%;
    }
</style>
<div id="Cyber_Security_Home_Carousel" class="carousel slide mb-3" data-bs-ride="carousel">
    <div class="carousel-inner">
        <div class="carousel-item active" data-bs-interval="3000">
            <img src="~/images/Cyber_Security_1.jpeg" class="d-block w-100" alt="Cyber_Security_1">
        </div>
        <div class="carousel-item" data-bs-interval="3000">
            <img src="~/images/Cyber_Security_2.jpeg" class="d-block w-100" alt="Cyber_Security_2">
        </div>
        <div class="carousel-item" data-bs-interval="3000">
            <img src="~/images/Cyber_Security_3.jpeg" class="d-block w-100" alt="Cyber_Security_3">
        </div>
        <div class="carousel-item">
            <img src="~/images/Cyber_Security_4.jpeg" class="d-block w-100" alt="Cyber_Security_4">
        </div>
    </div>
</div>